{
  "name": "reach-quantity",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "npm run jest:unit:ci && npm run build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "jest:unit": "jest",
    "jest:unit:ci": "env NODE_ENV=test jest --ci",
    "cypress": "cypress open",
    "cy-e2e:run": "cypress run --e2e",
    "cy-e2e:run:focus": "cypress run --e2e --env tags=@focus",
    "cy-comp:run": "cypress run --component",
    "local:check": "npm run lint && npm run cy-comp:run && npm run cy-e2e:run"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.3",
    "@mui/x-date-pickers": "^5.0.12",
    "@next/font": "13.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@vercel/analytics": "^0.1.6",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "idb": "^7.1.1",
    "luxon": "^3.2.0",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.41.5",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/luxon": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "cypress": "^12.3.0",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "^21.1.0",
    "ts-jest": "^29.0.5"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true
  }
}
